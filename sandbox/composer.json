{
    "name": "vaimo/composer-patches-sandbox",
    "type": "library",
    "license": "MIT",
    "description": "Sandbox module for testing out patch applier functionality",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "composer/composer": "^1.0.0",
        "vaimo/composer-patches": "*",
        "vaimo/composer-patches-target1": "1.0.0",
        "vaimo/composer-patches-target2": "1.2.0",
        "vaimo/composer-patches-target3": "1.2.3"
    },
    "config": {
        "platform": {
            "php": "5.3.9"
        }
    },
    "authors": [
        {
            "name": "Allan Paiste",
            "email": "allan.paiste@vaimo.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "path",
            "url": "modules/composer-patches",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "modules/composer-patches-target*",
            "options": {
                "symlink": false
            }
        }
    ],
    "extra": {
        "patches-search": "patches"
    }
}
